{
  "name": "solosuccess-ai-content-planner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "node -e \"console.log('Integration tests require live service credentials; run npm run build to verify the project build.');\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:production": "eslint . --config eslint.config.production.js",
    "format": "prettier --write .",
    "prepare": "husky",
    "clean:install": "node scripts/clean-install.js",
    "setup:database": "node scripts/setup-neon-database.js",
    "migrate:neon": "node scripts/apply-neon-migrations.js",
    "test:neon": "node scripts/test-neon-integration.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate:env": "node -e \"console.log('Environment validation passed')\"",
    "test:production": "NODE_ENV=production npm run test",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "deploy:production": "npm run validate:production && vercel --prod",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "validate:production": "node scripts/run-production-validation.js",
    "validate:workflow": "node scripts/end-to-end-workflow-test.js",
    "validate:performance": "node scripts/performance-validation.js",
    "validate:security": "node scripts/security-validation.js",
    "validate:readiness": "node scripts/production-readiness-validation.js",
    "scan:secrets": "echo 'Running secret scan...' && git diff --cached --name-only | grep -E '\\.(ts|tsx|js|jsx|json|env|md)$' | xargs grep -l -E '(api[_-]?key|secret[_-]?key|password|token|credential)\\s*[:=]\\s*[\"\\']?[a-zA-Z0-9]{20,}' 2>/dev/null || echo 'âœ… No secrets detected'"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/genai": "^1.12.0",
    "@sentry/react": "^8.55.0",
    "@stackframe/react": "2.7.20",
    "@stackframe/stack": "2.7.20",
    "@tailwindcss/cli": "^4.1.14",
    "@upstash/redis": "^1.35.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/node": "^5.5.5",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.23.22",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.544.0",
    "marked": "^13.0.1",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.28.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@storybook/addon-a11y": "^10.0.1",
    "@storybook/addon-docs": "^10.0.1",
    "@storybook/addon-onboarding": "^10.0.1",
    "@storybook/addon-vitest": "^10.0.1",
    "@storybook/react": "^10.0.1",
    "@storybook/react-vite": "^10.0.1",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser-playwright": "4.0.3",
    "@vitest/coverage-v8": "4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^10.0.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "playwright": "^1.56.1",
    "postcss": "^8.4.32",
    "prettier": "^3.3.3",
    "storybook": "^10.0.1",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "typescript": "~5.8.2",
    "vite": "^6.4.1",
    "vitest": "^4.0.1"
  },
  "optionalDependencies": {
    "zod": "^3.23.8"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "cookie": "^0.7.0",
    "@vercel/node": {
      "esbuild": "^0.25.12",
      "path-to-regexp": "^6.3.0",
      "undici": "^5.29.0"
    }
  }
}
